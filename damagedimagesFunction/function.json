{
  "scriptFile": "__init__.py",
  "entryPoint": "main",
  "bindings": [
    {
      "type": "eventGridTrigger",
      "direction": "in",
      "name": "inputEvent"
    },
    {
      "type": "blob",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "name": "inputBlob",
      "path": "{data.url}"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "direction": "out",
      "databaseName": "damageDB",
      "collectionName": "damageCollection",
      "connectionStringSetting": "CosmosConnection"
    }
  ]
}

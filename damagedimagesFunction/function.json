{
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "incoming-images/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "databaseName": "damageDB",
      "collectionName": "results",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDBConnection",
      "direction": "out"
    }
  ]
}
